    </xs:restriction>
  </xs:simpleType>
  
  <xs:attributeGroup name="UserRepresentationIDAttributes">
    <xs:attribute name="V6_3DEXP_PLMID"         type="xs:string"  />
    <xs:attribute name="REVISION"               type="xs:string"  />
    <!-- V6_3DEXP_PHYSID required only for Export. Usefull to export the geometry stl/std -->
    <xs:attribute name="V6_3DEXP_PHYSID"         type="xs:string"  />
  </xs:attributeGroup>

	<xs:complexType name="RotaryProfile">
		<xs:sequence>
			<xs:element name="Origin" type="Mfg3DPoint"/>
			<xs:element name="XAxis"  type="Mfg3DVector"/>
			<xs:element name="YAxis"  type="Mfg3DVector"/>
			<xs:element name="GeomElement" minOccurs="1" maxOccurs="unbounded" >
				<xs:complexType>
					<xs:attribute name="geomType" type="GeomType" use="required" />
					<xs:attribute name="endX" type="xs:double" use="required" />
					<xs:attribute name="endY" type="xs:double" use="required" />
					<xs:attribute name="startX" type="xs:double" use="required" />
					<xs:attribute name="startY" type="xs:double" use="required" />
					<xs:attribute name="centerX" type="xs:double" use="optional" /> <!-- Only for arc GeomType -->
					<xs:attribute name="centerY" type="xs:double" use="optional" /> <!-- Only for arc GeomType -->
					<xs:attribute name="orien" type="xs:integer" use="optional" />  <!-- Only for arc GeomType -->
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="GeomType">
		<xs:restriction base = "xs:string">
			<xs:enumeration value="line"/>
			<xs:enumeration value="arc"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="ShankGageInsert">
    <xs:attribute name="MFG_INSERT_TYPE"        type="xs:string"  use="required" />
    <xs:attribute name="MFG_ANGLE_FROM_INSERT"  type="xs:string"  />
    <xs:attribute name="MFG_LGTH_FROM_INSERT"   type="xs:string"  />
    <xs:attribute name="MFG_INSERT_SHIFT"       type="xs:string"  />
  </xs:complexType>

  <xs:complexType name="InsertHolderGageInsert">
    <xs:attribute name="MFG_MASTER_INSERT_IDENTIFICATION" type="xs:string"  />
    <xs:attribute name="MFG_INSERT_INTERFACE_CODE"        type="xs:string"  />
    <xs:attribute name="MFG_CUTTING_EDGE_ANGLE"           type="xs:string"        />
    <xs:attribute name="MFG_INSERT_TYPE"                  type="MFG_INSERT_TYPE"  use="required" />
    <xs:attribute name="MFG_ANGLE_FROM_INSERT"            type="xs:string"        />
    <xs:attribute name="MFG_LGTH_FROM_INSERT"             type="xs:string"        />
    <xs:attribute name="MFG_INSERT_SHIFT"                 type="xs:string"        />
    <xs:attribute name="MFG_CUTTING_ITEM_COUNT"           type="xs:integer"       use="required" />
    <xs:attribute name="MFG_INSERT_REF"                   type="xs:boolean"       />
  </xs:complexType>

  <xs:complexType name="InsertHolderGageInserts">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="InsertHolderGageInsert" type="InsertHolderGageInsert" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="MfgAttachments">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="MfgAttachment">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="MFG_ATTACHMENT_TYPE"    type="xs:string" />
            <xs:element name="MFG_ATTACHMENT_SUBTYPE" type="xs:string" minOccurs="1" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="MfgToolCorrectors">
    <xs:sequence>
      <xs:element name="MfgToolCorrector" minOccurs="1" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attribute name="MFG_SITE_TYP"              type="MFG_SITE_TYP"  />
          <xs:attribute name="MFG_SITE_NUM"              type="xs:integer" />
          <xs:attribute name="MFG_LNG_NUM"               type="xs:integer" />
          <xs:attribute name="MFG_RAD_NUM"               type="xs:integer" />
          <xs:attribute name="MFG_CMP_TL_DIAM"           type="xs:string" />
          <xs:attribute name="MFG_MCD_SIMULATION_LENGTH" type="xs:string" />
          <xs:attribute name="MFG_MCD_SIMULATION_RADIUS" type="xs:string" />
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="MfgAssemblyCorrectors">
    <xs:sequence>
      <xs:element name="MfgAssemblyCorrector" minOccurs="1" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attribute name="MFG_SITE_TYP" type="MFG_SITE_TYP"  use="required"/>
          <xs:attribute name="MFG_LNG_NUM"  type="xs:integer" use="required"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="MultiResourceAssemblyCorrectors">
    <xs:sequence>
      <xs:element name="MfgAssemblyCorrector" minOccurs="1" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attribute name="MFG_SITE_TYP" type="MFG_SITE_TYP"  use="required"/>
          <xs:attribute name="MFG_LNG_NUM"  type="xs:integer" use="required"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="NAME" type="xs:string" use="required"/>
  </xs:complexType>

  <xs:complexType name="MfgAssemblyCorrectorsInMultiResourceAssembly">
    <xs:sequence>
      <xs:element name="MfgMCAssemblyCorrector" type="MultiResourceAssemblyCorrectors" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="MfgFSConfiguration">
    <xs:attribute name = "MFG_FS_KEY1"               type="xs:string"  use="required" />
    <xs:attribute name = "MFG_FS_KEY2"               type="xs:string"  use="required" />
    <xs:attribute name = "MFG_FS_KEY3"               type="xs:string"  use="required" />
    <xs:attribute name = "MFG_FS_LINEAR_SPINDLE"     type="xs:boolean" use="required"/>
    <xs:attribute name = "MFG_FS_VC"                 type="xs:string" />
    <xs:attribute name = "MFG_FS_VC_SPINDLE"         type="xs:string" />
    <xs:attribute name = "MFG_FS_LINEAR_VF"          type="xs:boolean" use="required"/>
    <xs:attribute name = "MFG_FS_VF"                 type="xs:string" />
    <xs:attribute name = "MFG_FS_VF_TOOTH"           type="xs:string" />
    <xs:attribute name = "MFG_FS_MAX_MIL_LENGTH"     type="xs:string" />
    <xs:attribute name = "MFG_FS_MAX_MIL_TIME"       type="xs:string" />
    <xs:attribute name = "MFG_FS_MAX_COOLNT_SNTX"    type="xs:string"  />
    <xs:attribute name = "MFG_FS_MAX_FEEDRATE"       type="xs:string" />
    <xs:attribute name = "MFG_FS_MAX_CHIP_THICKNESS" type="xs:string" />
    <xs:attribute name = "MFG_FS_MAX_VOL_RATE"       type="xs:string" />
    <xs:attribute name = "MFG_FS_MAX_POWER"          type="xs:string" />
    <xs:attribute name = "MFG_FS_BENDING_FORCE"      type="xs:string" />
  </xs:complexType>

  <xs:complexType name="MfgFSMillingConfiguration">
    <xs:complexContent>
      <xs:extension base="MfgFSConfiguration">
        <xs:attribute name = "MFG_FS_PLUNGE_VF"      type="xs:string" />
        <xs:attribute name = "MFG_FS_PLUNGE_ANG"     type="xs:string" />
        <xs:attribute name = "MFG_FS_AP"             type="xs:string" />
		<xs:attribute name = "MFG_FS_AE"             type="xs:string" />
		<xs:attribute name = "MFG_FS_APPROACH_VF"    type="xs:string" />
		<xs:attribute name = "MFG_FS_RETRACT_VF"    type="xs:string" />
	  </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="MfgFSAxialConfiguration">
    <xs:complexContent>
      <xs:extension base="MfgFSConfiguration">
        <xs:attribute name = "MFG_FS_PLUNGE_VF"          type="xs:string" />
        <xs:attribute name = "MFG_FS_DEPTH"              type="xs:string" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="MfgFSLatheConfiguration">
    <xs:complexContent>
      <xs:extension base="MfgFSConfiguration">
        <xs:attribute name="MFG_FS_MAX_DEPTH"          type="xs:string" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="MfgFSMillingConfigurations">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="MfgFSMillingConfiguration" type="MfgFSMillingConfiguration" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="MfgFSAxialConfigurations">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="MfgFSAxialConfiguration" type="MfgFSAxialConfiguration" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="MfgLatheFSConfigurations">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="MfgFSLatheConfiguration" type="MfgFSLatheConfiguration" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Mfg3DPoint">
    <xs:attribute name="x"  type="xs:double"  use="required" />
    <xs:attribute name="y"  type="xs:double"  use="required" />
    <xs:attribute name="z"  type="xs:double"  use="required" />
  </xs:complexType>

  <xs:complexType name="Mfg3DVector">
    <xs:attribute name="i"  type="xs:double"  use="required" />
    <xs:attribute name="j"  type="xs:double"  use="required" />
    <xs:attribute name="k"  type="xs:double"  use="required" />
  </xs:complexType>

  <xs:complexType name="MfgAxisSystem">
    <xs:sequence>
      <xs:element name="Origin" type="Mfg3DPoint"  />
      <xs:element name="XAxis"  type="Mfg3DVector" />
      <xs:element name="YAxis"  type="Mfg3DVector" />
      <xs:element name="ZAxis"  type="Mfg3DVector" />
    </xs:sequence>

    <xs:attribute name="NAME" type="xs:string"/>
  </xs:complexType>


  <xs:complexType name="MfgToolAssemblyUserRepresentations">
    <xs:sequence>
      <!-- This Representation is created to support userreps for Mill and Drill ToolAssembly aggregating insert holder (Not for solid tool). 
    It is supposed to be a Cut type userrepresentation (rotary or static). 
  -->
      <xs:element name="CutTypeUserRepresentation" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <!--NewAxisSystem for Export XML -->
            <xs:element name="NewAxisSystem" type="MfgAxisSystem" minOccurs="0" maxOccurs="unbounded" />
			  
			  <!--Following 4 elements are for Export XML only -->
			  <xs:element name="Origin" type="MfgAxisSystem" minOccurs="0" maxOccurs="1" />
			  <xs:element name="WorkpieceSidePort" type="MfgAxisSystem" minOccurs="0" maxOccurs="1" />
			  <xs:element name="RotaryProfile" type="RotaryProfile" minOccurs="0" maxOccurs="1" />

		  </xs:sequence>
          
          <xs:attribute name="PATH"            type="xs:string" />
          <xs:attributeGroup ref="UserRepresentationIDAttributes"/>
          <xs:attribute name="USE_TYPE"     type="UserRepUseType" use="required" />
          <xs:attribute name="WorkpieceSidePort" type="xs:string"  />
          <!-- PositioningAxis specifies the name of the axis under user rep.
         The user rep will be positioned by matching the PositioningAxis under user rep with workpiece side axis of default product.
  -->
          <xs:attribute name="PositioningAxis"   type="xs:string"  />

        </xs:complexType>
      </xs:element>


    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="MfgMillToolUserRepresentations">
    <xs:sequence>

      <xs:element name="CutTypeUserRepresentation" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <!--NewAxisSystem for Import XML only -->
            <xs:element name="NewAxisSystem" type="MfgAxisSystem" minOccurs="0" maxOccurs="unbounded" />
            
            <!--Following 4 elements are for Export XML only -->
            <xs:element name="Origin" type="MfgAxisSystem" minOccurs="0" maxOccurs="1" />
            <xs:element name="WorkpieceSidePort" type="MfgAxisSystem" minOccurs="0" maxOccurs="1" />
            <xs:element name="MachineSidePort" type="MfgAxisSystem" minOccurs="0" maxOccurs="1" />
            <xs:element name="RotaryProfile" type="RotaryProfile" minOccurs="0" maxOccurs="1" />
          </xs:sequence>

          <xs:attribute name="PATH"            type="xs:string" />
          <xs:attributeGroup ref="UserRepresentationIDAttributes"/>
          <xs:attribute name="USE_TYPE"     type="UserRepUseType" use="required" />
          <xs:attribute name="WorkpieceSidePort" type="xs:string"  />
          <!-- PositioningAxis specifies the name of the axis under user rep.
       This axis will be used to position the user rep part under resource.
       The user rep will be positioned by matching the PositioningAxis under user rep with workpiece side axis of default product.
    -->
          <xs:attribute name="PositioningAxis"   type="xs:string"  />

        </xs:complexType>
      </xs:element>

      <xs:element name="NoCutTypeUserRepresentation" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
              <!--NewAxisSystem for Export XML -->
              <xs:element name="NewAxisSystem" type="MfgAxisSystem" minOccurs="0" maxOccurs="unbounded" />
            
            <!--Following 4 elements are for Export XML only -->
            <xs:element name="Origin" type="MfgAxisSystem" minOccurs="0" maxOccurs="1" />
            <xs:element name="WorkpieceSidePort" type="MfgAxisSystem" minOccurs="0" maxOccurs="1" />
            <xs:element name="MachineSidePort" type="MfgAxisSystem" minOccurs="0" maxOccurs="1" />
            <xs:element name="RotaryProfile" type="RotaryProfile" minOccurs="0" maxOccurs="1" />
           
            <xs:element name="InsertPort" minOccurs="0" maxOccurs="unbounded">
              <xs:complexType>
                
                <!--Following sequence are for Export XML only to give coordinates of insert port axis -->
                <xs:sequence>
                  <xs:element name="Origin" type="Mfg3DPoint"  />
                  <xs:element name="XAxis"  type="Mfg3DVector" />
                  <xs:element name="YAxis"  type="Mfg3DVector" />
                  <xs:element name="ZAxis"  type="Mfg3DVector" />
                </xs:sequence>
                
                <xs:attribute name="PORT_NAME"  type="xs:string" use="required" />
                <xs:attribute name="PORT_AXIS"  type="xs:string" use="required" />
              </xs:complexType>
            </xs:element>

          </xs:sequence>

          <xs:attribute name="PATH"            type="xs:string"    />
          <xs:attributeGroup ref="UserRepresentationIDAttributes"/>
          <xs:attribute name="USE_TYPE"     type="UserRepUseType" use="required" />
          <xs:attribute name="MachineSidePort"   type="xs:string"  />
          <!-- PositioningAxis specifies the name of the axis under user rep.
       This axis will be used to position the user rep part under resource.
       The user rep will be positioned by matching the PositioningAxis under user rep with machine side axis of default product.
    -->
          <xs:attribute name="PositioningAxis"   type="xs:string"  />
        </xs:complexType>
      </xs:element>

    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="MfgShankUserRepresentations">
    <xs:sequence>

      <xs:element name="NoCutTypeUserRepresentation" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>

          <xs:sequence>
            <!--NewAxisSystem for Import XML -->
            <xs:element name="NewAxisSystem" type="MfgAxisSystem" minOccurs="0" maxOccurs="unbounded" />

            <!--Following 3 elements are for Export XML only -->
            <xs:element name="Origin" type="MfgAxisSystem" minOccurs="0" maxOccurs="1" />
            <xs:element name="WorkpieceSidePort" type="MfgAxisSystem" minOccurs="0" maxOccurs="1" />
            <xs:element name="MachineSidePort" type="MfgAxisSystem" minOccurs="0" maxOccurs="1" />
          </xs:sequence>

          <xs:attribute name="PATH"              type="xs:string"  />
          <xs:attributeGroup ref="UserRepresentationIDAttributes"/>
          <xs:attribute name="USE_TYPE"          type="UserRepUseType"  use="required" />
          <xs:attribute name="WorkpieceSidePort" type="xs:string"  />
          <xs:attribute name="MachineSidePort"   type="xs:string"  />

          <!-- Specify any one of the following attributes in order to position the user rep part/product under the resource.
         Its value represents the name of the axis under user rep that will be used to position the user rep under resource.
       
